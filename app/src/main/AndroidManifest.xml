<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.applus">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="com.google.android.providers.gsf.permission.READ_GSERVICES" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />
    <uses-permission android:name="android.permission.CAMERA" />
    <uses-permission android:name="android.permission.VIBRATE" />
    <uses-permission android:name="android.permission.ACCESS_WIFI_STATE" />
    <uses-permission android:name="android.permission.BLUETOOTH" />
    <uses-permission android:name="android.permission.READ_PHONE_STATE" />

    <uses-feature android:name="android.hardware.camera" />
    <uses-feature android:name="android.hardware.camera.autofocus" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">

        <activity
            android:name="com.applus.gestionordenes.MainActivity"
            android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.applus.vistas.operario.OperarioActivity"
            android:label="@string/title_activity_operario"
            android:screenOrientation="portrait"></activity>
        <activity
            android:name="com.applus.vistas.admin.AdminActivity"
            android:label="@string/title_activity_admin"></activity>
        <activity
            android:name="com.applus.vistas.operario.brigada.BrigadaActivity"
            android:label="@string/title_activity_brigada_form"
            android:screenOrientation="portrait"></activity>
        <activity
            android:name="com.applus.vistas.operario.brigada.BrigadaListado"
            android:label="@string/title_activity_brigada"
            android:screenOrientation="portrait"></activity>
        <activity
            android:name="com.applus.vistas.operario.totalizadores.TotalizadoresActivity"
            android:label="@string/title_activity_totalizador_form"
            android:screenOrientation="portrait"></activity>
        <activity
            android:name="com.applus.vistas.operario.totalizadores.TotalizadorListado"
            android:label="@string/title_activity_totalizador"
            android:screenOrientation="portrait"></activity>
        <activity
            android:name="com.applus.vistas.operario.novedad.NovedadActivity"
            android:label="@string/title_activity_novedad"
            android:screenOrientation="portrait"></activity>
        <activity
            android:name="com.applus.vistas.operario.novedad.NovedadListado"
            android:label="@string/title_activity_novedad_form"
            android:screenOrientation="portrait"></activity>
        <activity
            android:name="com.applus.vistas.operario.censo.CensoActivity"
            android:label="@string/title_activity_censo_form"
            android:screenOrientation="portrait"></activity>
        <activity
            android:name="com.applus.vistas.operario.censo.CensoListado"
            android:label="@string/title_activity_censo"
            android:screenOrientation="portrait"></activity>
    </application>
</manifest>
